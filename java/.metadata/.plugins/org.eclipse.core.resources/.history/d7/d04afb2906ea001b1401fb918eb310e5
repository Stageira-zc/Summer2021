package com.aicity.smartparkingapplication.TileComponent;

import java.util.ArrayList;
import java.util.List;

public class TileCollectionChanges {
    public List<TileRegion> entered = new ArrayList<>();
    public List<TileRegion> exited = new ArrayList<>();
    public List<TileRegion> remained = new ArrayList<>();

    public TileCollectionChanges(List<TileRegion> entered, List<TileRegion> exited, List<TileRegion> remained){
        this.entered = entered;
        this.exited = exited;
        this.remained = remained;
    }
}
