package scanner;

public class switch_case {

	public static void main(String[] args) {
		int option = 0;
		switch (option) {
		case 1: {
			System.out.println("Selected 1");
			
		}
		case 2: {
			System.out.println("2");
		}
		case 3: {
			System.out.println("Selected 2, 3");
			
		}
		default: {
			System.out.println("Not selected");
			
		}
		}
	}

}
